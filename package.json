{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "htmlparser",
    "description": "#### api documentation for  [htmlparser (v1.7.7)](http://github.com/tautologistics/node-htmlparser)  [![npm package](https://img.shields.io/npm/v/npmdoc-htmlparser.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-htmlparser) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-htmlparser.svg)](https://travis-ci.org/npmdoc/node-npmdoc-htmlparser)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "htmlparser": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-htmlparser",
    "keywords": [
        "documentation",
        "htmlparser"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_htmlparser.js",
    "name": "npmdoc-htmlparser",
    "nameAlias": "npmdoc_htmlparser",
    "nameOriginal": "npmdoc-htmlparser",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-htmlparser.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
